<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title>Secuence list</title>
</head>
<body>
	<table>
		<thead>
			<tr>
				<th>Secuence</th>
				<th>Requester</th>
				<th>Solution</th>
			</tr>
		</thead>
		<tbody>
			{{#each list}}
				<tr>
					<td>{{secuence}}</td>
					<td>{{requester}}</td>
					{{#if_exists solution}}
              			<td>{{solution}}</td>
            		{{else}}
             			<td>Not solved</td>
            		{{/if_exists}}
					<td><a href="/secuencePage?secuence={{secuence}}">secuence page</a></td>
				</tr>
			{{/each}}
		</tbody>
	</table>
</body>
</html>